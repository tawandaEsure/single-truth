{
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "no-var": 2,
    "spaced-comment": [2, "always"]
  },
  "env": {
    "es6": true,
    "node": true,
    "protractor": true,
    "jasmine": true,
    "mocha": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 9
  }
}
